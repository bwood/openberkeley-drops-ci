<?php
/**
 * @file
 * Code for the Open Berkeley Theme feature.
 */

include_once 'openberkeley_theme.features.inc';

/**
 * Implements of hook_ctools_plugin_directory()
 */
function openberkeley_theme_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'panels' && $plugin_type == 'layouts') {
    return "plugins/$plugin_type";
  }
}
